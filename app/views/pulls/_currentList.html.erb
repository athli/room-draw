<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Number</th>
      <th>Room</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @pull.room_assignments.each do |room_assignment| %>
      <tr>
        <td><%= student_name(room_assignment) %></td>
        <td><%= student_number(room_assignment) %></td>
        <td><%= room_assignment.room.name %></td>
        <td><%= link_to 'Delete', room_assignment_path(room_assignment, :redirect_path => redirect_path), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>

<h3>Message: </h3> <%= @pull.message %>
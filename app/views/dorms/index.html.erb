<p id="notice"><%= notice %></p>

<h1>Dorms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dorms.each do |dorm| %>
      <tr>
        <td><%= dorm.name %></td>
        <td><%= link_to 'Show', dorm, :"data-turbolinks" => false %></td>
        <td><%= link_to 'Edit', edit_dorm_path(dorm) %></td>
        <td><%= link_to 'Destroy', dorm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dorm', new_dorm_path %>
